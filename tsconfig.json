{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["jest", "node", "@wdio/types", "webdriverio", "@wdio/mocha-framework"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "*/.ts",
    "*/.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/**/*", "__tests__/**/*", "testing-library.d.ts"
  ]
}
